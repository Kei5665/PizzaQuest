<%= link_to monster_path(monster.id), class: "list-group-item-action" do %>
  <li class="list-group-item d-flex justify-content-between align-items-start">
    <div class="ms-2 me-auto">
      <p><%=monster.name%></p>
      <p>lelel:<%= monster.level%></p>
    </div>
    <div class="monster-img">
      <span class="badge bg-primary rounded-pill">モンスター画像</span>
    </div>
  </li>
<%end%>