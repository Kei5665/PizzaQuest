<div class="col-sm-12 col-lg-4 mb-3">
  <div class="card">
    <div class="card-body">
      <h4 class="card-title">
        <%= gram.gram%>
      </h4>
      <ul class="list-inline">
        <li class="list-inline-item">ID:<%= gram.id%></li>
      </ul>
      <%= button_to '削除',admin_gram_path(gram.id), method: :delete, data: { confirm: '削除しますか？' }, class: 'btn btn-sm btn-danger'%>
    </div>
  </div>
</div>